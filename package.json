{
  "name": "remix-app-template",
  "private": true,
  "description": "- [Krabuu Docs](https://krabuu.com)",
  "license": "ISC",
  "sideEffects": false,
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "yarn tsc && cross-env NODE_ENV=production remix build",
    "build:deploy": "sh deploy.sh",
    "dev": "dotenv -e .env.dev -- npm-run-all build --parallel \"dev:*\"",
    "dev:build": "cross-env NODE_ENV=development remix dev",
    "dev:serve": "cross-env NODE_ENV=development nodemon --watch build server.js",
    "dev:db": "npx prisma studio",
    "dev:docker-db": "docker-compose up",
    "db:reset:dev": "dotenv -e .env.dev npx prisma migrate reset",
    "db:seed:dev": "yarn db:reset:dev && dotenv -e .env.dev npx prisma db seed",
    "db:migrate:create": "dotenv -e .env.dev npx prisma migrate dev --name",
    "db:migrate": "run-p db:migrate:*",
    "db:migrate:dev": "dotenv -e .env.dev npx prisma migrate dev",
    "db:migrate:test": "dotenv -e .env.test -- npx prisma db push --accept-data-loss",
    "db:gen:migrate": "dotenv -e .env.dev npx prisma migrate dev --name",
    "start": "cross-env NODE_ENV=production node ./build/index.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:local": "run-s db:migrate:test test:coverage:disabled",
    "test:ci": "run-s db:migrate:test npm run test:coverage:enabled",
    "test:coverage:enabled": "dotenv -e .env.test -- vitest --coverage",
    "test:coverage:disabled": "dotenv -e .env.test -- vitest"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.145.0",
    "@heroicons/react": "^2.0.4",
    "@prisma/client": "^3.14.0",
    "@radix-ui/react-avatar": "^1.0.0",
    "@radix-ui/react-label": "^1.0.0",
    "@radix-ui/react-popover": "1.0.0",
    "@remix-run/express": "^1.14.1",
    "@remix-run/node": "^1.14.1",
    "@remix-run/react": "^1.14.1",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.1.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "eslint": "^8.10.0",
    "express": "^4.17.1",
    "framer-motion": "^6.3.4",
    "isbot": "^3.6.6",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.1",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-aria": "^3.23.1",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.2.2",
    "remix-auth-form": "^1.1.2",
    "rollbar": "^2.25.0",
    "superagent": "^7.1.3",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@faker-js/faker": "^6.3.1",
    "@remix-run/dev": "^1.14.1",
    "@remix-run/eslint-config": "^1.14.1",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/crypto-js": "^4.1.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.30",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/superagent": "^4.1.15",
    "@vitejs/plugin-react": "^3.0.1",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.5",
    "c8": "^7.11.2",
    "dotenv": "^16.0.1",
    "eslint-plugin-storybook": "^0.5.11",
    "jsdom": "^19.0.0",
    "msw": "^0.39.2",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.7",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "prisma": "^3.14.0",
